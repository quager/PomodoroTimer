<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainBackground" Color="DimGray"/>
    <SolidColorBrush x:Key="WindowHeaderColor" Color="Gray"/>
    <SolidColorBrush x:Key="TextControlForeground" Color="White"/>
    
    <SolidColorBrush x:Key="ControlBackgroundColorKey" Color="#FF646464"/>
    <SolidColorBrush x:Key="ControlColorKey" Color="#FF505050"/>
    <SolidColorBrush x:Key="SecondaryControlColorKey" Color="#FF202020"/>
    <SolidColorBrush x:Key="AccentControlColorKey" Color="#FFF5F5F5"/>
    <SolidColorBrush x:Key="DisabledControlColorKey" Color="Gainsboro"/>
    <SolidColorBrush x:Key="DisabledControlTextColorKey" Color="#FFD0D0D0"/>
    <SolidColorBrush x:Key="ActiveControlColorKey" Color="#FF0085C0"/>
    <SolidColorBrush x:Key="ControlTextColorKey" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="HighlightColorKey" Color="#880096F0"/>
    <SolidColorBrush x:Key="HighlightTextColorKey" Color="Black"/>
    <SolidColorBrush x:Key="BorderBrushKey" Color="Silver" />
    <SolidColorBrush x:Key="ControlActiveColorKey" Color="#FFC0E0F0" />
    <SolidColorBrush x:Key="ControlBorderActiveColorKey" Color="#FF50A0F0" />

</ResourceDictionary>